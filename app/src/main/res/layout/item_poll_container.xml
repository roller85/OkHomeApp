<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:background="#ffffff"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <LinearLayout
        android:orientation="vertical"
        android:gravity="center_vertical"
        android:padding="14dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:textSize="18sp"
            android:textColor="@color/okHomeGrayDeep"
            android:id="@+id/itemPollContainer_tvTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:text="시발"/>

        <TextView
            android:textSize="13sp"
            android:textColor="@color/okHomeGray"
            android:id="@+id/itemPollContainer_tvSubTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" android:text="시발섭타이틀"/>


        <!-- 투표내용 들어올곳-->
        <LinearLayout
            android:id="@+id/itemPollContainer_vgPollItems"
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


        </LinearLayout>

    </LinearLayout>
</LinearLayout>
